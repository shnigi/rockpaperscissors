<?xml version='1.1' encoding='utf-8'?>
<widget id="com.kps.shnigi" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RockPaperScissorsLizardSpock</name>
    <description>
        Rock Paper Scissors Lizard Spock mobile game!
    </description>
     <author email="niki.ahlskog@gmail.com" href="http://nikiahlskog.com">
        Niki Ahlskog
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="orientation" value="portrait" />
    <preference name="android-minSdkVersion" value="14" />
    <platform name="android">
		<icon src="res/android/drawable-xhdpi/icon.png" density="" />
        <icon src="res/android/drawable-ldpi/icon.png" density="ldpi" />
        <icon src="res/android/drawable-mdpi/icon.png" density="mdpi" />
        <icon src="res/android/drawable-hdpi/icon.png" density="hdpi" />
        <icon src="res/android/drawable-xhdpi/icon.png" density="xhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
